{
  "appId": "com.yansd.music",
  "appName": "YanSD Music",
  "webDir": "dist",
  "server": {
    "androidScheme": "https",
    "hostname": "localhost",
    "iosScheme": "capacitor"
  },
  "android": {
    "minWebViewVersion": 60,
    "allowMixedContent": true,
    "captureInput": true,
    "webContentsDebuggingEnabled": false,
    "appendUserAgent": "YanSDMusic/1.0",
    "overrideUserAgent": "",
    "backgroundColor": "#1a1a1a",
    "toolbarColor": "#1a1a1a",
    "navigationBarColor": "#1a1a1a",
    "hideLogs": true,
    "loggingBehavior": "none",
    "mixedContentMode": "compatibility",
    "handleRequestsInWebView": true
  },
  "plugins": {
    "CapacitorHttp": {
      "enabled": true
    },
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#1a1a1a",
      "showSpinner": false,
      "androidSpinnerStyle": "small",
      "iosSpinnerStyle": "small",
      "spinnerColor": "#ffffff",
      "splashFullScreen": true,
      "splashImmersive": true
    },
    "StatusBar": {
      "style": "dark",
      "backgroundColor": "#1a1a1a"
    },
    "Keyboard": {
      "resize": "body",
      "style": "dark",
      "resizeOnFullScreen": true
    }
  }
}
